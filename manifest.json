{
  "name": "Mootwo Servers",
  "manifest_version": 2,
  "version": "0.2",
  "description": "Select trusted Mootwo servers with ease.",
  "content_scripts": [
    {
      "matches": ["http://moomoo.io/*"],
      "js": ["inject.js"]
    }
  ],
  "browser_action": {
    "default_popup": "pop.html"
  }
}
