{
  "name": "Mootwo Servers",
  "manifest_version": 2,
  "version": "1.1",
  "description": "Select trusted Mootwo servers with ease.",
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": ["http://moomoo.io/*"],
      "js": ["inject.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "pop.html",
    "default_icon": {
      "256": "icons/256.png",
      "128": "icons/128.png",
      "38": "icons/38.png",
      "19": "icons/19.png"
    }
  }
}
